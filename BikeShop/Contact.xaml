        <TextBox Height="35" Margin="107,36,23,0" TextWrapping="Wrap" VerticalAlignment="Top" FontSize="15"/>
        <TextBox Margin="107,82,23,43.6" TextWrapping="Wrap" FontSize="15"/>
        <Label>
            <local:Car Speed = "30.0" Color="Blue">
                <local:Car.Driver>
                    <local:Human FirstName="hanjin" HasDrivingLicense="True" />
                </local:Car.Driver>
